{
  "name": "@benefex/inactivity-logout",
  "version": "1.0.0-beta-0",
  "description": "A vanilla JS library to automatically logout users after a specified time",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start",
    "test-via-saucelabs": "cross-env NODE_ENV=test SAUCELABS=true karma start",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --content-base src/ --host 0.0.0.0",
    "build": "cross-env NODE_ENV=distribution webpack",
    "document": "cross-env npm run typedoc -- --out ./docs src/inactivity-logout.ts --theme minimal --mode file --readme none",
    "prepublish": "cross-env npm run build",
    "postinstall": "cross-env npm run typings install",
    "typings": "typings",
    "typedoc": "typedoc"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "cross-env": "1.0.8",
    "jasmine": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-sauce-launcher": "0.3.1",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "ts-loader": "0.8.1",
    "tslint": "3.11.0",
    "typedoc": "0.4.5",
    "typescript": "1.8.10",
    "typings": "1.3.3",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
