{
  "name": "inactivity-countdown-timer",
  "version": "1.0.0-beta-2",
  "description": "A plain JS (Typescript) module that will countdown and timeout when users are inactive/idle. ",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "keywords": [
    "inactivity",
    "inactive",
    "countdown",
    "timer",
    "idle"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start",
    "test-via-saucelabs": "cross-env NODE_ENV=test SAUCELABS=true karma start",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --content-base src/ --host 0.0.0.0",
    "build": "cross-env NODE_ENV=distribution webpack",
    "document": "cross-env npm run typedoc -- --out ./docs src/inactivity-countdown-timer.ts --theme minimal --mode file --excludeNotExported",
    "prepublish": "cross-env npm run build",
    "commit": "git-cz",
    "typings": "typings",
    "typedoc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Benefex/inactivity-countdown-timer.git"
  },
  "bugs": {
    "url": "https://github.com/Benefex/inactivity-countdown-timer/issues"
  },
  "homepage": "https://github.com/Benefex/inactivity-countdown-timer",
  "author": "Benefex",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "commitizen": "2.8.6",
    "cross-env": "1.0.8",
    "cz-conventional-changelog": "1.2.0",
    "jasmine": "2.4.1",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-sauce-launcher": "0.3.1",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "ts-loader": "0.8.1",
    "tslint": "3.11.0",
    "typedoc": "0.4.5",
    "typescript": "1.8.10",
    "typings": "1.3.3",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
