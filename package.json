{
  "name": "inactivity-countdown-timer",
  "version": "1.0.0-beta-2",
  "description": "A plain JS (Typescript) module that will countdown and timeout when users are inactive/idle. ",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "keywords": [
    "inactivity",
    "inactive",
    "countdown",
    "timer",
    "idle"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start",
    "test-via-saucelabs": "cross-env NODE_ENV=test SAUCELABS=true karma start",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --host 0.0.0.0",
    "build": "cross-env NODE_ENV=distribution webpack",
    "document": "cross-env npm run typedoc -- --out ./docs src/inactivity-countdown-timer.ts --theme minimal --mode file --excludeNotExported",
    "commit": "git-cz",
    "typedoc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Benefex/inactivity-countdown-timer.git"
  },
  "bugs": {
    "url": "https://github.com/Benefex/inactivity-countdown-timer/issues"
  },
  "homepage": "https://github.com/Benefex/inactivity-countdown-timer",
  "author": "Benefex",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.4.1",
    "@types/node": "^12.7.9",
    "commitizen": "^2.10.1",
    "cross-env": "^1.0.8",
    "cz-conventional-changelog": "^1.2.0",
    "jasmine": "^3.5.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-sauce-launcher": "^2.0.2",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "ts-loader": "^6.2.0",
    "tslint": "^5.2.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
